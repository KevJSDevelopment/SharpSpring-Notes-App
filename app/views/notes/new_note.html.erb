<!-- app/views/posts/new.html.erb -->
  <%= form_for @note do |f| %>
    <%= f.label :title, "Title" %>
    <%= f.text_field :title , class: 'form-control'%>
    <br>
    <%= f.label :description, "Description:" %>
    <%= f.text_area :description , class: 'form-control'%>
    <%= f.submit %>
  <% end %>
